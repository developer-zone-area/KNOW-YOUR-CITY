{
  "version": 2,
  "buildCommand": "cd client && npm run vercel-build",
  "outputDirectory": "client/build",
  "installCommand": "cd client && npm install",
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
